<h1> Music </h1>


<table>
    <tr>
        <th>Song Description</th>
    </tr>
    <% @songs.each do |song|%>
        <tr>
        <td><%= song.title%></td>
        <td><%= song.artist%></td>
        <td><%= song.genre%></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure of deleting the selected?' } %></td>
      </tr>
    <%end%>
    

</table>
<button type="button" class="btn btn-info"><%= link_to 'New Song', new_song_path %></button>
